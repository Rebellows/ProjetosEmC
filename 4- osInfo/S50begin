#!/bin/sh
case "$1" in
    start)
        /usr/bin/hello & 
        /usr/bin/server &
        /usr/bin/osInfo
        ;;
    stop)
        exit 1
        ;;
    *)
        exit 1
        ;;
esac
exit 0

